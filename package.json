{
  "name": "executecmdupdater",
  "version": "1.2.2",
  "description": "一个用于更新mc存档中/execute指令的工具",
  "main": "main.js",
  "dependencies": {
    "fs-extra": "^11.2.0",
    "leveldb-zlib": "^1.2.0",
    "parsenbt-js": "^1.1.2"
  },
  "author": "HTMonkeyG",
  "license": "MIT",
  "bin": "./main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pkg . --out-path=dist/"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "targets": [
      "node18"
    ],
    "assets": [
      "node_modules/leveldb-zlib",
      "node_modules/fs-extra",
      "node_modules/parsenbt-js"
    ],
    "outputPath": "dist"
  }
}